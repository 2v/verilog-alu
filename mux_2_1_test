    %store/vec4 v0x555782497d40_0, 0, 1;
    %store/vec4 v0x555782497c80_0, 0, 1;
    %delay 2, 0;
    %load/vec4 v0x555782498230_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x555782497c80_0;
    %store/vec4 v0x555782497e10_0, 0, 1;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x555782497d40_0;
    %store/vec4 v0x555782497e10_0, 0, 1;
T_0.3 ;
    %load/vec4 v0x555782498190_0;
    %load/vec4 v0x555782497e10_0;
    %cmp/e;
    %jmp/0xz  T_0.4, 4;
    %load/vec4 v0x555782497f80_0;
    %addi 1, 0, 32;
    %store/vec4 v0x555782497f80_0, 0, 32;
    %jmp T_0.5;
T_0.4 ;
